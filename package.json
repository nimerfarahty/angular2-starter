{
  "name": "angular2-starter",
  "version": "2.0.0-alpha.1",
  "author": "Antony Budianto",
  "description": "Angular 2 Starter with extensible features",
  "main": "src/tmp/app/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonybudianto/angular2-starter.git"
  },
  "engines": {
    "node": ">=4.3",
    "npm": "~3.0.0"
  },
  "scripts": {
    "postinstall": "typings install",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "posttest": "npm run coverage",
    "e2e": "gulp e2e",
    "env": "gulp env",
    "ngc": "ngc -p tsconfig-aot.json",
    "serve": "lite-server -c config/browser-sync/bs-config.js",
    "build": "npm run ngc && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "serve-build": "npm run build && npm run serve",
    "clean": "gulp clean",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i report/coverage.json -o report/html-report -t html -e node_modules,tests,karma.entry.ts",
    "coverage:report": "istanbul report",
    "coveralls": "coveralls < ./report/remap/lcov.info"
  },
  "keywords": [
    "angular2",
    "starter",
    "seed",
    "gulp",
    "webpack",
    "extensible",
    "systemjs",
    "systemjs builder",
    "travis",
    "karma",
    "jasmine",
    "istanbul"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonybudianto/angular2-starter/issues"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/compiler-cli": "0.6.2",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.0.0",
    "@types/lodash": "^4.14.34",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "0.0.28",
    "core-js": "^2.4.1",
    "del": "~2.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "font-awesome": "^4.6.3",
    "glob": "^7.0.6",
    "gulp": "^3.9.1",
    "gulp-protractor": "^3.0.0",
    "gulp-template": "^4.0.0",
    "jasmine-core": "~2.5.0",
    "jquery": "^3.1.0",
    "karma": "~1.2.0",
    "karma-coverage": "~1.1.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.0.2",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "lodash": "^4.15.0",
    "phantomjs-prebuilt": "^2.1.12",
    "remap-istanbul": "~0.6.4",
    "require-dir": "~0.3.0",
    "run-sequence": "~1.2.2",
    "rxjs": "5.0.0-beta.12",
    "to-string-loader": "^1.1.4",
    "typescript": "2.0.2",
    "typings": "^1.3.2",
    "yargs": "^5.0.0",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^1.0.0",
    "karma-remap-istanbul": "^0.2.1",
    "lite-server": "^2.2.2",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.6.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
